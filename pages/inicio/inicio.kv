#:import PageInicio pages.inicio.inicio

<PageInicio>:
    id: pagina_inicio
    name: "screen_inicio"

    AnchorLayout:
        anchor_x: "center"
        anchor_y: "top"
        BoxLayout:
            orientation: "vertical"
            size_hint_y: .6
            pos_hint: {"center_y": .8}

            ScrollView:
                do_scroll_x: False
                MDList:
                    id: list

            MDRaisedButton:
                text: "Logout"
                size_hint: None, None
                pos_hint: {"center_x": .5}
                on_release: root.logout()